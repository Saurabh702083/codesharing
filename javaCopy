CREATE TABLE PAYOUT_INFO(
    PI_ID                      RAW(16) DEFAULT SYS_GUID() PRIMARY KEY,
    PAYOUT_REPORT_ID           RAW(16),
    TXN_MIS_REPORT_ID          RAW(16),
    MERCHANT_MIS_REPORT_ID     RAW(16),
    REFUND_MIS_REPORT_ID       RAW(16),
    CREATED_DATE               NUMBER NOT NULL,
    UPDATED_DATE               NUMBER
    );

